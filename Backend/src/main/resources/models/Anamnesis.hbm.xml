<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.cvut.fit.genepi.models.AnamnesisDAO" table="anamnesis">
		<id name="id" type="int">
			<column name="id" precision="6" scale="0" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="date" type="date">
			<column name="date" length="7" not-null="true" />
		</property>
		<property name="doctorId" type="int">
			<column name="doctor_id" length="6" not-null="false" />
		</property>
		<property name="added" type="date">
			<column name="added" length="7" not-null="true" />
		</property>
		<property name="beginningEpilepsy" type="date">
			<column name="beginning_epilepsy" length="7" not-null="false" />
		</property>

		<property name="firstFever" type="int">
			<column name="first_fever" precision="1" scale="0" not-null="false" />
		</property>
		<property name="infantileSpasm" type="int">
			<column name="infantile_spasm" precision="1" scale="0"
				not-null="false" />
		</property>
		<property name="specificSyndromeIdcom" type="int">
			<column name="specific_syndrome_idcom" precision="6" scale="0"
				not-null="false" />
		</property>
		<property name="epilepsyInFamily" type="int">
			<column name="epilepsy_in_family" precision="1" scale="0"
				not-null="false" />
		</property>
		<property name="prenatalRisk" type="int">
			<column name="prenatal_risk" precision="1" scale="0" not-null="false" />
		</property>

		<property name="fibrilConvulsions" type="int">
			<column name="fibril_convulsions" precision="1" scale="0"
				not-null="false" />
		</property>
		<property name="inflammationCns" type="int">
			<column name="inflammation_cns" precision="1" scale="0"
				not-null="false" />
		</property>
		<property name="injuryCns" type="int">
			<column name="injury_cns" precision="1" scale="0" not-null="false" />
		</property>
		<property name="operationCns" type="int">
			<column name="operation_cns" precision="1" scale="0" not-null="false" />
		</property>
		<property name="earlyPmdRetardation" type="int">
			<column name="early_pmd_retardation" precision="1" scale="0"
				not-null="false" />
		</property>
		<property name="nonCnsComorbidity" type="string">
			<column name="non_cns_comorbidity" length="400" not-null="false" />
		</property>
		<property name="comment" type="string">
			<column name="comment" length="400" not-null="false" />
		</property>
		<property name="deleted" type="int">
			<column name="deleted" precision="4" scale="0" not-null="false" />
		</property>
		<property name="patientId" type="int">
			<column name="patient_id" precision="6" scale="0" not-null="false" />
		</property>
		<property name="addUserId" type="int">
			<column name="add_user_id" precision="6" scale="0" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
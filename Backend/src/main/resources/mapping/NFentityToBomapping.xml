<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <date-format>yyyy-MM-dd</date-format>
    </configuration>

    <mapping>
        <class-a>cz.cvut.fit.genepi.dataLayer.entity.card.NeurologicalFindingEntity</class-a>
        <class-b>cz.cvut.fit.genepi.businessLayer.BO.NeurologicalFindingBO</class-b>
        <field>
            <a>date</a>
            <b>date</b>
        </field>
        <field custom-converter=
                       "cz.cvut.fit.genepi.businessLayer.converter.hemisphereDominanceConverter">
            <a>hemisphereDominance</a>
            <b>hemisphereDominance</b>
        </field>
    </mapping>
</mappings>